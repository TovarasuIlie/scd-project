<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="UI.Pages.ProfilePage"
             Title="ProfilePage"
             xmlns:viewmodel ="clr-namespace:UI.ViewModel"
         x:DataType="viewmodel:ProfileViewModel">
    <Grid RowDefinitions="Auto, Auto, Auto, Auto" Padding="100">
        <Label Text="{x:Binding LoggedUser.Email}" Grid.Row="0" Grid.Column="0"></Label>
        <Label Text="{x:Binding LoggedUser.Id}" Grid.Row="1" Grid.Column="0"></Label>
        <Label Text="{x:Binding LoggedUser.Name}" Grid.Row="2" Grid.Column="0"></Label>
        <Label Text="{x:Binding LoggedUser.Jwt}" Grid.Row="3" Grid.Column="0"></Label>
    </Grid>
</ContentPage>